@model List<Page>





    <h5 class="text-white">Navigation</h5>
    <ul class="list-unstyled text-muted">
        @foreach (var item in Model)
        {
            <li class="nav-item">
            <a id="LinkColor" asp-controller="Home" asp-action="Index" asp-route-slug="@item.Slug">@item.Title</a>
            </li>
        }
    </ul>


